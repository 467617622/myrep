<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.scf.systemmng.data">
	<class
		name="TblCommLog"
		table="TBL_COMM_LOG"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="flowNo"
			column="FLOW_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="businessNo"
			column="BUSINESS_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="txnCode"
			column="TXN_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="txdate"
			column="TXDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="txtime"
			column="TXTIME"
			type="timestamp"
			not-null="true"
			length="11"
		/>
		<property
			name="brcode"
			column="BRCODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="tlrno"
			column="TLRNO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="source"
			column="SOURCE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="dest"
			column="DEST"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="txnStatus"
			column="TXN_STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="resCode"
			column="RES_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="rcvMsg"
			column="RCV_MSG"
			type="string"
			not-null="false"
		/>
		<property
			name="sendMsg"
			column="SEND_MSG"
			type="string"
			not-null="false"
		/>
		<property
			name="miscflgs"
			column="MISCFLGS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="timestamp"
			not-null="false"
			length="11"
		/>
		<property
			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdFunc"
			column="LAST_UPD_FUNC"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdDate"
			column="LAST_UPD_DATE"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="rspCode"
			column="RSP_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="txnAmt"
			column="TXN_AMT"
			type="big_decimal"
			not-null="false"
			length="16"
		/>

	</class>
</hibernate-mapping>