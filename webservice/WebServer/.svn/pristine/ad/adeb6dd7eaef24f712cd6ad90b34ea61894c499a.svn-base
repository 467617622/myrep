		<property name="fciCode" column="FCI_CODE" type="string"
			not-null="false" length="10" />

		<property name="isIntl" column="IS_INTL" type="string"
			not-null="false" length="1" />

		<property name="isFeePay" column="IS_FEE_PAY" type="string"
