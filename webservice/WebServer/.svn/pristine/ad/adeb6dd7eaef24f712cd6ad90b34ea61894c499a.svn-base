		<property name="blackFlag" column="BLACK_FLAG" type="string"
			not-null="false" length="1" />

		<property name="brcode" column="BRCODE" type="string"
			not-null="false" length="20" />

		<property name="createDate" column="CREATE_DATE" type="java.util.Date"
			not-null="false" />

		<property name="insureFlag" column="INSURE_FLAG" type="string"
			not-null="false" length="1" />

		<property name="factorFlag" column="FACTOR_FLAG" type="string"
			not-null="false" length="1" />

		<property name="importantFlag" column="IMPORTANT_FLAG" type="string"
