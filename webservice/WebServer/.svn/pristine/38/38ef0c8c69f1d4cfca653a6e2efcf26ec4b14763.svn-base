package com.huateng.scf.systemmng.data.base;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

 
public class BaseTblDebtExtInfo implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2614573704044454156L;
	private String id;
	private String custcdSaller;
	private String cnameBuyer;
	private String custcdBuyer;
	private String cnameSeller;
	private String appNum;
	private Date billDate;
	private String debtContNo; 
	private BigDecimal billAmount;
	private BigDecimal creditPeriod;
	private String status;

	 
	
	public BaseTblDebtExtInfo(String id) {
		this.setId(id);
	}
	 
	 
	public BaseTblDebtExtInfo() {
 
	}

	public String getCustcdSaller() {
		return custcdSaller;
	}
	public void setCustcdSaller(String custcdSaller) {
		this.custcdSaller = custcdSaller;
	}
	public String getCnameBuyer() {
		return cnameBuyer;
	}
	public void setCnameBuyer(String cnameBuyer) {
		this.cnameBuyer = cnameBuyer;
	}
	public String getCustcdBuyer() {
		return custcdBuyer;
	}
	public void setCustcdBuyer(String custcdBuyer) {
		this.custcdBuyer = custcdBuyer;
	}
	public String getCnameSeller() {
		return cnameSeller;
	}
	public void setCnameSeller(String cnameSeller) {
		this.cnameSeller = cnameSeller;
	}
	public String getAppNum() {
		return appNum;
	}
	public void setAppNum(String appNum) {
		this.appNum = appNum;
	}
	 
	public String getDebtContNo() {
		return debtContNo;
	}
	public void setDebtContNo(String debtContNo) {
		this.debtContNo = debtContNo;
	}
	public BigDecimal getBillAmount() {
		return billAmount;
	}
	public void setBillAmount(BigDecimal billAmount) {
		this.billAmount = billAmount;
	}
 
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	 
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}


	public BigDecimal getCreditPeriod() {
		return creditPeriod;
	}


	public void setCreditPeriod(BigDecimal creditPeriod) {
		this.creditPeriod = creditPeriod;
	}


	public Date getBillDate() {
		return billDate;
	}


	public void setBillDate(Date billDate) {
		this.billDate = billDate;
	}
		 
}
